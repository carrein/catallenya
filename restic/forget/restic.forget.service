[Unit]
Description=Weekly Restic Forget
After=network-online.target
OnSuccess=restic.forget.success.ntfy.service
OnFailure=restic.forget.failure.ntfy.service

[Service]
Type=oneshot
User=root
Environment="RCLONE_CONFIG=/zpool/catallenya/rclone/.rclone.conf"
ExecStart=/zpool/catallenya/restic/forget/restic.forget.sh